{
    "manifest_version": 3,
    "name": "Element Data Collector",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "host_permissions": [
      "https://*/*"
    ],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }, 
    "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["content.js","popup.js"]
      }
  ]
  }
  